/**
 * \brief Enumeration of valid key mods (possibly OR'd together).
 */
extern(C)
  enum SDL_Keymod {
    KMOD_NONE     = 0x0000,
    KMOD_LSHIFT   = 0x0001,
    KMOD_RSHIFT   = 0x0002,
    KMOD_LCTRL    = 0x0040,
    KMOD_RCTRL    = 0x0080,
    KMOD_LALT     = 0x0100,
    KMOD_RALT     = 0x0200,
    KMOD_LGUI     = 0x0400,
    KMOD_RGUI     = 0x0800,
    KMOD_NUM      = 0x1000,
    KMOD_CAPS     = 0x2000,
    KMOD_MODE     = 0x4000,
    KMOD_RESERVED = 0x8000
  } 
}

immutable KMOD_CTRL  = SDL_Keymod.KMOD_LCTRL | SDL_Keymod.KMOD_RCTRL;
immutable KMOD_SHIFT = SDL_Keymod.KMOD_LSHIFT | SDL_Keymod.KMOD_RSHIFT;
immutable KMOD_ALT   = SDL_Keymod.KMOD_LALT | SDL_Keymod.KMOD_RALT;
immutable KMOD_GUI   = SDL_Keymod.KMOD_LGUI | SDL_Keymod.KMOD_RGUI;

